<h1>Listing partners</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Category</th>
    <th>Logo</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @partners.each do |partner| %>
  <tr>
    <td><%= partner.name %></td>
    <td><%= partner.category %></td>
    <td><%= partner.logo %></td>
    <td><%= partner.url %></td>
    <td><%= link_to 'Show', partner %></td>
    <td><%= link_to 'Edit', edit_partner_path(partner) %></td>
    <td><%= button_to 'Destroy', partner, method: :delete, data: { confirm: 'Are you sure?' }, :class => "button radius" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Partner', new_partner_path %>
