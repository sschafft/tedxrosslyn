    <!-- Cover Image -->
    <div id="container">
        <div id="slides">
            <%= image_tag "example-slide-1.jpg" %>
            <%= image_tag "example-slide-2.jpg" %>
            <%= image_tag "example-slide-3.jpg" %>
            <%= image_tag "example-slide-4.jpg" %>
            <%= image_tag "test.jpg" %>
        </div>
        <div id="center">
            <h1>Imagining the Future</h1>
            <h2>June 9, 2013</h2>
        </div>
    </div>

    <section id="main">
    	<%= link_to event_path do %>
        <div id="" class="tab">
            <h2>EVENT</h2>
        </div>
        <% end %>
        <%= link_to speakers_path do %>
        <div id="" class="tab">
            <h2>SPEAKERS</h2>
        </div>
        <% end %>
        <%= link_to partners_path do %>
        <div id="" class="tab">
            <h2>PARTNERS</h2>
        </div>
        <% end %>
        <%= link_to about_path do %>
        <div id="" class="tab">
            <h2>ABOUT</h2>
        </div>
        <% end %>
    </section>

<script>
    $(function() {
      $('#slides').slidesjs({
        width: 940,
        height: 528,
        play: {
          active: false,
          auto: true,
          interval: 4000,
          swap: true
        }
      });
    });
  </script>